@model IEnumerable<OutcomesFirst.Models.Placement>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.PlacementFirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlacementLastName)
            </th>
       
            <th>
                @Html.DisplayNameFor(model => model.PlacementPlacementStartDate)
            </th>
     
                   
            <th>
                @Html.DisplayNameFor(model => model.PlacementGender.GenderName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlacementService.ServiceName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlacementLocalAuthority)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
           
            <td>
                @Html.DisplayFor(modelItem => item.PlacementFirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlacementLastName)
            </td>
         
                      <td>
                @Html.DisplayFor(modelItem => item.PlacementPlacementStartDate)
            </td>
           
            <td>
                @Html.DisplayFor(modelItem => item.PlacementGender.GenderName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlacementService.ServiceName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PlacementLocalAuthority.LocalAuthorityName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PlacementId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PlacementId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PlacementId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
